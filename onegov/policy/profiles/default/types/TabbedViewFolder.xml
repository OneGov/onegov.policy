<?xml version="1.0"?>
<object name="TabbedViewFolder">

    <action
        action_id="events"
        title="Event"
        category="tabbedview-tabs"
        condition_expr="here/@@module-active/meeting"
        url_expr="string:${object_url}?view=events"
        visible="True">
        <permission value="View"/>
    </action>

    <action
        action_id="tasks"
        title="Tasks"
        category="tabbedview-tabs"
        condition_expr="here/@@module-active/task"
        url_expr="string:${object_url}?view=tasks"
        visible="True">
        <permission value="View"/>
    </action>

</object>
